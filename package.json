{
    "name": "vscode-tmgrammar-test-windows-bug-reproduction",
    "displayName": "vscode-tmgrammar-test windows bug reproduction",
    "description": "Example of VSCode extension that reproduces a bug on windows in vscode-tmgrammar-test",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lang",
                "aliases": [
                    "lang"
                ],
                "extensions": [
                    ".lang"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lang",
                "scopeName": "source.lang",
                "path": "./syntaxes/lang.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "vscode-tmgrammar-test": "0.0.5"
    },
    "scripts": {
        "test": "vscode-tmgrammar-snap --grammar syntaxes/lang.tmLanguage.json --scope source.lang --testcases 'test/**/*.lang'"
    }
}
